	_G.AutoRandomFruits = Value
    spawn(function()
		while wait() do
			pcall(function()
				if _G.AutoRandomFruits then
					game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Cousin", "Buy")
				end
			end)
		end
	end)
