if World2 or World3 then
    local Race = Server:Channel("Race","http://www.roblox.com/asset/?id=15118939041")
	if World2 then
		local RaceLable = Race:Label("")
		spawn(function()
			while game:GetService("RunService").Heartbeat:wait() do
				RaceLable:Refresh("Race V.1-3")
			end
		end)
		Race:Toggle("Auto Evolution Race V2",_G.AutoEvolutionRaceV2,function(Value)
			_G.AutoEvolutionRaceV2 = Value
			if Value == false then
				wait(0.3)
				TP(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
			end
			spawn(function()
				pcall(function()
					while wait() do
						if _G.AutoEvolutionRaceV2 then
							if not game:GetService("Players").LocalPlayer.Data.Race:FindFirstChild("Evolved") then
								if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","1") == 0 then
									TP(CFrame.new(-2780, 73, -3574))
									if (Vector3.new(-2780, 73, -3574) - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 15 then
										game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","2")
									end
								elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","1") == 1 then
									pcall(function()
										if not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Flower 1") and not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Flower 1") then
											TP(game:GetService("Workspace").Flower1.CFrame)
										elseif not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Flower 2") and not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Flower 2") then
											TP(game:GetService("Workspace").Flower2.CFrame)
										elseif not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Flower 3") and not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Flower 3") then
											if game:GetService("Workspace").Enemies:FindFirstChild("Zombie") then
												for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
													if v.Name == "Zombie" then
														repeat wait()
															Haki()
															EquipWeapon()
															PosMon = v.HumanoidRootPart.CFrame
															TP(v.HumanoidRootPart.CFrame * CFrame.new(0,40,0))
															v.HumanoidRootPart.CanCollide = false
															v.Humanoid.WalkSpeed = 0
															v.Head.CanCollide = false               
															v.HumanoidRootPart.Size = Vector3.new(60,60,60)
															game:GetService'VirtualUser':CaptureController()
															game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
															BringMob = true
															FastAttack = true
														until game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Flower 3") or not v.Parent or v.Humanoid.Health <= 0 or not _G.AutoEvolutionRaceV2
														BringMob = false
														FastAttack = false
													end
												end
											else
												TP(CFrame.new(-5686, 49, -853))
											end
										end
									end)
								elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","1") == 2 then
									game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","3")
								end
							end
						end
					end
				end)
			end)
		end)
	elseif World3 then
		local MoonLable = Race:Label("")
		spawn(function()
			while game:GetService("RunService").Heartbeat:wait() do
				pcall(function()
					if game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149431" then
						MoonLable:Refresh("Status :  [Full Moon 100 %]")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149052" then
						MoonLable:Refresh("Status :  [Full Moon 75 %]")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709143733" then
						MoonLable:Refresh("Status :  [Full Moon 50 %]")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709150401" then
						MoonLable:Refresh("Status :  [Full Moon 25 %]")
					elseif game:GetService("Lighting").Sky.MoonTextureId=="http://www.roblox.com/asset/?id=9709149680" then
						MoonLable:Refresh("Status :  [Full Moon 15 %]")
					else
						MoonLable:Refresh("Status :  [Full Moon 0 %]")
					end
				end)
			end
		end)
		Race:Button("Teleport To Temple of Time", function()
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(28286, 14897, 103))
		end)
		Race:Button("Unlock Lever", function()
			if game:GetService("Workspace").Map["Temple of Time"].Lever.Prompt:FindFirstChild("ProximityPrompt") then
				game:GetService("Workspace").Map["Temple of Time"].Lever.Prompt:FindFirstChild("ProximityPrompt"):Remove()
			else
				require(game:GetService("ReplicatedStorage").Notification).new("<Color=Green> < Unlocked > <Color=/>"):Display()
			end
		end)
		Race:Button("Teleport To Race Door", function()
			if game.Players.LocalPlayer.Data.Race.Value == "Human" then
				TP2(CFrame.new(29237, 14890, -207))
			elseif game.Players.LocalPlayer.Data.Race.Value == "Mink" then
				TP2(CFrame.new(29021, 14890, -379))
			elseif game.Players.LocalPlayer.Data.Race.Value == "Fishman" then
				TP2(CFrame.new(28224, 14890, -211))
			elseif game.Players.LocalPlayer.Data.Race.Value == "Skypiea" then
				TP2(CFrame.new(28967, 14918, 234))
			elseif game.Players.LocalPlayer.Data.Race.Value == "Ghoul" then
				TP2(CFrame.new(28673, 14890, 455))
			elseif game.Players.LocalPlayer.Data.Race.Value == "Cybrog" then
				TP2(CFrame.new(28492, 14895, -422))
			end
		end)
		Race:Line()
		local AncientLable = Race:Label("")
		spawn(function()
			while game:GetService("RunService").Heartbeat:wait() do
				AncientLable:Refresh("Ancient Quest")
			end
		end)
		Race:Button("Teleport To Ancient One", function()
			TP2(CFrame.new(28974, 14890, -117))
		end)
		Race:Line()
		local MysticIslandLable = Race:Label("")
		spawn(function()
			while game:GetService("RunService").Heartbeat:wait() do
				if game:GetService('Workspace').Map:FindFirstChild('MysticIsland') then
					MysticIslandLable:Refresh("Mirage : Spawned")
				elseif game:GetService('Workspace')["_WorldOrigin"].Locations:FindFirstChild('MysticIsland') then
					MysticIslandLable:Refresh("Mirage : Ready to Spawn")
				else
					MysticIslandLable:Refresh("Mirage : Not Spawned")
				end
			end
		end)
		Race:Toggle("Teleport to Mirage Island",_G.AutoMirageIsland,function(Value)
			_G.AutoMirageIsland = Value
			spawn(function()
				while wait() do
					if _G.AutoMirageIsland then
						pcall(function()
							if game:GetService("Workspace").Map:FindFirstChild("MysticIsland") then
								TP(game:GetService("Workspace").Map:FindFirstChild("MysticIsland").HumanoidRootPart.CFrame * CFrame.new(0,500,-100))
							end
						end)
					end
				end
			end)
		end)
		Race:Toggle("Teleport To Gear",_G.TeleportToGear,function(Value)
			_G.TeleportToGear = Value
			spawn(function()
				while wait() do
					if _G.TeleportToGear then
						TP(game:GetService("Workspace").Map.MysticIsland:FindFirstChildOfClass("MeshPart").CFrame)
					end
				end
			end)
		end)
	end
end

