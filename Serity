DevilFruit:Toggle("Auto Store Devil Fruit",_G.AutoStoreDevilFruit,function(Value)
	_G.AutoStoreDevilFruit = Value
	spawn(function()
		pcall(function()
			while wait(.1) do
				if _G.AutoStoreDevilFruit then
					if game:GetService("Players").LocalPlayer.Character:FindFirstChild(FruitList) or game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(FruitList) then
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StoreFruit",FruitTable)
					end
				end
			end
		end)
	end)
end)

